var r1 = 10 / 3;
console << "type=" << typeof(r1) << ", value=" << r1 << "\n";
// output type=integer, value=3

var r2 = 10 % 3;
console << "type=" << typeof(r2) << ", value=" << r2 << "\n";
// output type=integer, value=1

var r3 = 10 / 3.;
console << "type=" << typeof(r3) << ", value=" << r3 << "\n";
// output type=float, value=3.333333
